{
    "editor.rulers": [
        80,
        120
    ],
    "editor.detectIndentation": false,
    "files.insertFinalNewline": true,
    "files.associations": {
        "Makefile.*": "makefile",
        "*.mk": "makefile",
        "ipsec.h": "c",
        "pkt.h": "c",
        "frag.h": "c",
        "sixlowpan.h": "c",
        "sched.h": "c",
        "hdr.h": "c",
        "esp.h": "c",
        "config.h": "c",
        "debug.h": "c",
        "ipv6.h": "c",
        "ext.h": "c",
        "array": "c",
        "string_view": "c",
        "initializer_list": "c",
        "ranges": "c",
        "utility": "c",
        "hmac.h": "c",
        "ipsec_ts.h": "c",
        "ipsec_enums.h": "c",
        "ipsec_db.h": "c",
        "nettype.h": "c",
        "udp.h": "c",
        "rh.h": "c",
        "pktbuf.h": "c",
        "addr.h": "c"
    },
    "[shellscript][c][cpp]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
    },
    "[makefile]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
    },
    "C_Cpp.default.defines": ["MODULE_GNRC_IPV6_IPSEC"]
}
